#!/bin/sh
#
# spr1-squashball-create
#
# usage: echo $SECRET | spr1-squashball-create /input/directory /output.sqr1 [/reference.spr1 ...]
#

ME=$(realpath "$0")
BIN=$(dirname "$ME")
PROJECT=$(dirname "$BIN")

JAR="$PROJECT/out/artifacts/spr_squashball_jar/spr-squashball.jar"

exec java -jar "$JAR" "$@"
